# Configurações do Servidor
NODE_ENV=development
PORT=3000

# Configurações do Banco de Dados
DATABASE_URL="file:./dev.db"

# Configurações de Segurança
JWT_SECRET=your-super-secret-jwt-key-change-in-production
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configurações de Log
LOG_LEVEL=info

# Configurações da API Pix do Banco do Brasil
BB_API_BASE_URL=https://api.bb.com.br/pix/v1
BB_CLIENT_ID=your-bb-client-id
BB_CLIENT_SECRET=your-bb-client-secret
BB_DEVELOPER_APPLICATION_KEY=your-bb-developer-application-key
BB_PIX_KEY=your-bb-pix-key

# Configurações do PIX (futuras integrações)
PIX_API_URL=https://api.pix.example.com
PIX_API_KEY=your-pix-api-key 